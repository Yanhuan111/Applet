page{
    width: 100%;
    height: 100%;
}
 .container{
    width: 100%;
    height: 100%;
    background-color: #f1f1f1;
}
.header{
    width: 670rpx;
    height: 235rpx;
    margin-top: -150rpx;
    border-radius: 10rpx;
    border: 1px solid #e1e1e1;
    background-color: #ffffff;
}
.time{
    width: 615rpx;
    height: 110rpx;
    margin: 0 auto;
    border-bottom: 1px solid #e1e1e1;
}
.remove{
    float: left;
    margin-top: 30rpx;
    font-size: 40rpx;
    font-weight: 400;
    color: #333333;
}
.now{
    float: right;
    margin-top: 30rpx;
    font-size: 35rpx;
    font-weight: 400;
    color: #f17c0e;
}
.address{
    margin: 0 auto;
    width: 625rpx;
    height: 120rpx;
    background-color: #ffffff;
}
.position{
    float: left;
    margin-top: 30rpx;
    width: 200rpx;
    height: 30rpx;
    font-size: 40rpx;
    font-weight: 400;
    color: #333333;
}
.site{
    float: right;
    margin-top: -55rpx;
    width: 460rpx;
    height: 125rpx;
}
.name{
    float: right;
    font-size: 30rpx;
    color: #333333;
    width: 460rpx;
    height: 50rpx;
    text-align: center;
    margin-top: 15rpx;
} 
.minaddress{
    float: right;
    font-size: 25rpx;
    color: #e1e1e1;
    width: 460rpx;
    height: 75rpx;
    text-align: center;
    margin-top: 15rpx;
} 
.detail{
    margin-right: 480rpx;
    margin-top: 40rpx;
    font-size: 40rpx;
    color: #333333;
}
.orderList{
    width: 670rpx;
    height: 100%;
    display: flex;
    box-sizing: border-box;
    border-radius: 10rpx;
}
::-webkit-scrollbar
{
  width: 0;
  height: 0;
  color: transparent;
}
.order{
  display: flex;
  height: 125rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  border: 1px solid #e1e1e1;
  background-color: #fff;
}
.order-left{
  flex: 1
}
.order-right{
  flex: 1
}
.order-name{
    color: #333333;
    height: 50%;
    line-height: 90rpx;
    padding-left: 20rpx;
}
.property{
    height: 50%;
    padding-left: 20rpx;
}
.property1{
    font-size: 25rpx;
    color: #e1e1e1;
}
.property2{
    font-size: 25rpx;
    color: #e1e1e1;
}
.property3{
    font-size: 25rpx;
    color: #e1e1e1;
}
.order-price{
    position: absolute;
    margin-top: 45rpx;
    margin-left: 30rpx;
    font-size: 35rpx;
}
.order-num{
    position: absolute;
    margin-top: 45rpx;
    margin-left: 600rpx;
    font-size: 35rpx;
}
.discount{
    position: relative;
    width: 670rpx;
    height: 200rpx;
    top: 40rpx;
    border-radius: 10rpx;
    border: 1px solid #e1e1e1;
    background-color: #fff;
}
.img1{
    position: relative;
    float: left;
    width:60rpx;
    height: 60rpx;
    text-align: center;
    top: 15rpx;
    margin-left: 20rpx;
}
.none{
    position: relative;
    float: right;
    font-size: 30rpx;
    font-weight: 400;
    color: #f17d10;
    top: 25rpx;
    margin-right: 20rpx;
}
.remark{
    position: relative;
    right: 280rpx;
    top: 50rpx;
    font-size: 40rpx;
    color: #333333;
}
.explain{
    position: relative;
    width: 670rpx;
    height: 250rpx;
    margin: 0 auto;
    top: 60rpx;
    border: 1px solid #e1e1e1;
    border-radius: 10rpx;
    background-color: #fff;
    font-size: 30rpx;
}
.footer{
    position: fixed;
    width: 100%;
    height: 110rpx;
    bottom: 0;
    left: 0;
    background-color: #ffffff;
    border-top: 1px solid #e1e1e1;
  }
.total{
    position: absolute;
    width: 120rpx;
    height: 110rpx;
    float: left;
    margin: 0;
  }
  .total_quantity{
    position: absolute;
    margin-top: 30rpx;
    margin-left: 65rpx;
    width: 35rpx;
    height: 35rpx;
    border-radius: 50%;
    text-align: center;
    background-color: #f17c0e;
    font-size: 30rpx;
    color: #ffffff;
  }
  .quantity{
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    margin-left: 30rpx;
    margin-top: 38rpx;
    background-color: #fff;
    text-align: center;
  }
  .sum{
    position: absolute;
    width: 90rpx;
    height: 30rpx;
    margin-top: 40rpx;
    margin-left: 130rpx;
    font-size: 40rpx;
    color: #f17c0e;
  }
  .account{
    position: absolute;
    width: 150rpx;
    height: 30rpx;
    margin-top: 40rpx;
    margin-left: 550rpx;
    font-size: 35rpx;
    color: #f17c0e;
  }
  .arrows{
    float: right;
    width: 45rpx;
    height: 45rpx;
    margin-top: 40rpx;
    margin-right: 30rpx;
  }
  .mainBox{
      width: 100%;
      height: 900rpx;
      background-color: #ffffff;
  }
   .bigBox{
     margin: 0;
     padding: 0;
     margin: 0 auto;
     background-color: #ffffff;
  }
  .top_box{
    margin: 0;
    padding: 0;
    height: 300rpx;
  }
  .close{
      color: #a0a0a0;
      font-size: 40rpx;
      font-weight: 300;
      position: absolute;
      top: 30rpx;
      left: 40rpx;
  }
  .pay_title{
      height: 80rpx;
      padding-top: 20rpx;
      border-bottom: 1px solid #ececec;
      text-align: center;
      font-weight: 500;
  }
  .sum_price{
      height: 80rpx;
      padding-top: 30rpx;
      text-align: center;
      font-size: 35rpx;
      font-weight: 500;
  }
  .totaledPrice{
    padding-top: 10rpx;
      height: 80rpx;
      text-align: center;
      font-size: 60rpx;
  }
  .mindle_box{
      margin: 0;
      padding: 0;
      margin: 0 auto;
      height: 300rpx;
  }
  .pay_way{
      margin: 0 auto;
      color: #a0a0a0;
      font-size: 30rpx;
      height: 80rpx;
      width: 700rpx;
      padding-left: 20rpx;
      padding-top: 30rpx;
      border-bottom: 1px solid #ececec;
  }
  .pay_choose{
      margin: 0 auto;
      height: 80rpx;
      width: 700rpx;
      display: flex;
      border-bottom: 1px solid #ececec;
  }
  .icon{
      height: 80rpx;
      width: 100rpx;
      text-align: center;
  }
  .weichat_pay{
      padding-top: 20rpx;
      padding-right: 35rpx;
      width: 50rpx;
      height: 45rpx;
  }
  .wechat{
      padding-top: 20rpx;
      font-size: 35rpx;
      font-weight: 500;
  }
  .pay_select{
      position: absolute;
      margin-left: 600rpx;
      margin-top: 20rpx;
      height: 45rpx;
      width: 45rpx;
      color:#f17c0e;
  }
  .under_box{
    margin: 0;
    padding: 0;
    margin: 0 auto;
    height: 300rpx;
  }
  .confirm{
      margin: 0 auto;
      margin-top: 130rpx;
      height: 100rpx;
      width: 600rpx;
      border-radius: 10rpx;
      background-color: #f17c0e;
  }
  .words{
      color: #ffffff;
      padding-left: 170rpx;
      padding-top: 25rpx;
  }
